    "@vue/cli-service": "^3.3.1",
    "ajv": "^6.7.0",
    "chance": "^1.0.18",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prismjs": "^1.15.0",
    "sass-loader": "^7.0.1",
    "typescript": "^3.2.4",
    "vue": "^2.5.22",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.5.22",
    "webpack-cli": "^3.2.1"
