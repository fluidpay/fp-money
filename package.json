{
  "name": "@gwservices/fp-money",
  "version": "2.0.3-next.5",
  "private": false,
  "description": "Input formatter and currency selector",
  "repository": {
    "type": "git",
    "url": "github.com/fluidpay/fp-money"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/fp-money.umd.js",
  "module": "./dist/fp-money.es.js",
  "browser": "./dist/fp-money.umd.js",
  "unpkg": "./dist/fp-money.umd.js",
  "style": "./dist/style.css",
  "scss": "./src/fp-money/fp-money.scss",
  "exports": {
    ".": {
      "import": "./dist/fp-money.es.js",
      "require": "./dist/fp-money.umd.js",
      "types": "./dist/fp-money.d.ts"
    },
    "./vue": {
      "import": "./dist/component.vue.js",
      "require": "./dist/component.vue.umd.cjs",
      "types": "./dist/component.vue.d.ts"
    },
    "./utils": {
      "import": "./dist/fp-money.es.js",
      "require": "./dist/fp-money.umd.js",
      "types": "./dist/helpers.d.ts"
    }
  },
  "scripts": {
    "dev": "vite --config=src/docs/vite.config.ts --port=1111",
    "format": "prettier --write --cache --parser typescript \"src/**/*.ts\"",
    "build": "vite build && rollup --config src/fp-money/rollup.config.ts --configPlugin typescript && npm run build:tsc",
    "build:tsc": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
    "build:docs": "vite --config=src/docs/vite.config.ts build",
    "test": "vitest run",
    "testui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "vitestwatch": "vitest watch",
    "deploy:next": "npm run build && npm version prerelease --no-git-tag-version --preid=next && npm publish"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/chance": "^1.1.6",
    "@types/prismjs": "^1.26.4",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/ui": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "chance": "^1.1.11",
    "eslint": "^8.2.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "jsdom": "^24.0.0",
    "prismjs": "^1.29.0",
    "rollup-plugin-scss": "^4.0.0",
    "sass": "^1.77.6",
    "sass-loader": "^14.2.1",
    "typescript": "^5.5.2",
    "vue": "^3.2.39",
    "vue-loader": "^17.1.1",
    "vue-router": "^4.1.5",
    "vue-template-compiler": "^2.7.10",
    "vue-tsc": "^2.0.22"
  }
}
