{
  "name": "fp-money",
  "version": "0.27.0",
  "private": false,
  "description": "FluidPay input formatter and currency selector",
  "repository": "github.com/fluidpay/fp-money",
  "license": "MIT",
  "main": "dist/fp-money.js",
  "types": "dist/fp-money.d.ts",
  "css": "dist/fp-money.css",
  "style": "dist/fp-money.css",
  "sass": "src/fp-money/fp-money.scss",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run docs && npm run library",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "iso": "node -e 'require(`./src/fp-money/iso.js`).outputUniqueCurrencies()'",
    "library": "rm -r dist && cd src/fp-money && webpack && cd ../../ && rm dist/fp-money-css.js",
    "docs": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/chance": "^1.1.0",
    "@types/mocha": "^9.1.1",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-plugin-unit-mocha": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/test-utils": "^2.0.2",
    "ajv": "^8.11.0",
    "chai": "^4.3.6",
    "chance": "^1.1.7",
    "eslint": "^8.23.1",
    "eslint-plugin-vue": "^9.5.0",
    "prismjs": "^1.29.0",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "typescript": "^4.8.3",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5",
    "vue-template-compiler": "^2.7.10",
    "webpack-cli": "^4.10.0"
  }
}
